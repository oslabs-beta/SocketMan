{
  "name": "realworld.svelte.dev",
  "version": "1.0.1-next.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "npm run prepare & vite dev",
    "build": "npm run smui-theme-light && npm run smui-theme-dark && vite build",
    "test": "vitest",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "preview": "vite preview",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "_preview": "vite preview",
    "svelte-check": "svelte-check"
  },
  "devDependencies": {
    "@mdi/js": "^7.0.96",
    "@smui-extra/accordion": "^6.1.1",
    "@smui/button": "^6.1.0",
    "@smui/fab": "^7.0.0-beta.0",
    "@smui/form-field": "^6.1.0",
    "@smui/icon-button": "^7.0.0-beta.0",
    "@sveltejs/adapter-netlify": "^1.0.0-next.83",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "^1.0.0-next.532",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.2",
    "marked": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "smui-theme": "^6.1.3",
    "svelte": "^3.49.0",
    "svelte-check": "^2.10.3",
    "tslib": "^2.4.0",
    "typescript": "~4.8.3",
    "vite": "^3.1.1",
    "vitest": "^0.24.5"
  },
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "express": "^4.18.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "socket.io-mock-ts": "^1.0.2"
  }
}
